<%= form_with(model: vehicle) do |form| %>
  <% if vehicle.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(vehicle.errors.count, "error") %> prohibited this vehicle from being saved:</h2>

      <ul>
        <% vehicle.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>

    <%= form.label :"charges Id", style: "display: block" %>
    <%= form.number_field :charges_id %>

    <%= form.label :"Name/Number", style: "display: block" %>
    <%= form.text_field :owner %>
  </div>
  <div>
    <%= form.label :"charges Id", style: "display: block" %>
    <%= form.number_field :charges_id %>

  </div>

  <div>
    <%= form.label :"Vehicle no", style: "display: block" %>
    <%= form.text_field :number %>
  </div>

  <div>
    <%= form.label :in_time, style: "display: block" %>
    <%= form.datetime_field :in_time %>
  </div>

  <div>
    <%= form.label :out_time, style: "display: block" %>
    <%= form.datetime_field :out_time %>
  </div>

  <div>
    <%= form.label :charges, style: "display: block" %>
    <%= form.number_field :charges %>
  </div>

  <div>
    <%= form.label :status, style: "display: block" %>
    <%= form.text_field :status %>
  </div>

  <div>
    <%= form.label :"user_id=#{current_user.id} ,name=#{current_user.firstName}", style: "display: block" %>
    <%= form.number_field :user_id,value: current_user.id ,:type =>"hidden"%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

