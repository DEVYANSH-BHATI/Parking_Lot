  <p style="color: green"><%= %></p>

  <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
      <%= render "shared/dashboard_header" %>
      <div class="app-main">
        <%= render "shared/dashboard_sidebar"%>
        <div class="app-main__outer">
          <div class="app-main__inner">
            <div class="app-page-title text-center"><strong>Parking Lot Dashboard</strong></div>
            <div class="row">
              <div class="col-md-12">
                <div class="main-card mb-3 card">
                  <div class="card-header">
                    <div class="row ">
                      <div class="col">
                        <button type="button" class="btn-shadow btn btn-outline-info"><%= link_to "Edit this vehicle", edit_vehicle_path(@vehicle) %> </button>
                      </div>
                      <div class="col">
                        <%= button_to "Delete this Vehicle", @vehicle, method: :delete, class: "btn-shadow btn btn-outline-info"%>
                      </div>
                      <%# <div class="col">
                        <button type="button" class="btn-shadow btn btn-outline-info"><%= link_to "Back to vehicles", vehicles_path %>
                        <%# </button> %>
                      <%# </div> %>

                    </div>
                  </div>
                  <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                      <thead>
                        <tr>
                          <th class="text-center">#</th>
                          <th class="text-center">Name/Number</th>
                          <th class="text-center">Type</th>
                          <th class="text-center">Vehicles No</th>
                          <th class="text-center">In time</th>
                          <th class="text-center">Out time</th>
                          <th class="text-center">Total Hours</th>
                          <th class="text-center">Charges</th>
                          <th class="text-center">Status</th>
                          <th class="text-center">Actions(User)</th>
                        </tr>
                      </thead>
                      <%= render @vehicle %>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


