<p style="color: green"><%= notice %></p>

<p style="color: green"><%= %></p>

<div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
  <div class="app-header header-shadow">
    <div class="app-header__logo">
      <div class="logo-sr"><strong>PARKING LOT</strong></div>

      <div class="header__pane ml-auto">
        <div>
          <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
            <span class="hamburger-box">
              <span class="hamburger-inner"></span>
            </span>
          </button>
        </div>
      </div>
    </div>
    <div class="app-header__mobile-menu">
      <div>
        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
          <span class="hamburger-box">
            <span class="hamburger-inner"></span>
          </span>
        </button>
      </div>
    </div>
    <div class="app-header__menu">
      <span>
        <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
          Admin
        </button>
      </span>
    </div>
    <div class="app-header__content">
      <div class="app-header-right">
        <a data-toggle="dropdown" class="p-0 btn">
          <div class="">
            Admin
          </div>
        </a>
        <div tabindex="-1" role="menu" aria-hidden="true" class="dropdown-menu dropdown-menu-right">
          <button type="button" tabindex="0" class="dropdown-item">User Account</button>
          <button type="button" tabindex="0" class="dropdown-item">Settings</button>
          <button type="button" tabindex="0" class="dropdown-item">
            <% if current_user %>
            <%= button_to "sign_out", destroy_user_session_path, method: :delete, class:"btn btn-danger" %>
            <% end %>
          </button>
        </div>
      </div>
    </div>
  </div>
  <div class="app-main">
    <div class="app-sidebar sidebar-shadow">

      <div class="scrollbar-sidebar">
        <div class="app-sidebar__inner">
          <ul class="vertical-nav-menu">
            <%= link_to "Dashboard", vehicles_path, class: "btn-shadow btn btn-outline-dark" %>

            <li class="app-sidebar__heading">
             <%= link_to "All vehicles", vehicles_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "Parked vehicles", vehicles_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "Left vehicles", vehicles_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "New vehicles", new_vehicle_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "Vehicles Charge", charges_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "New charge", new_charge_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
             <li class="app-sidebar__heading">
             <%= link_to "Update charge", charges_path, class: "btn-shadow btn btn-outline-info" %>
             </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="app-main__outer">
      <div class="app-main__inner">
        <div class="app-page-title text-center"><strong>Parking Lot Dashboard</strong></div>
        <div class="row">
          <div class="col-md-12">
            <div class="main-card mb-3 card">
              <div class="card-header">
                <div class="row">
                  <div class="col">
                    <button type="button" class="btn-shadow btn btn-outline-secondary">Name/Number</button>
                  </div>
                  <div class="col">
                    <button type="button" class="btn-shadow btn btn-outline-success">VehicleNo</button>
                  </div>
                  <div class="col">
                    <button type="button" class="btn-shadow btn btn-outline-danger">Bike</button>
                  </div>
                  <div class="col">
                    <button type="button" class="btn-shadow btn btn-outline-danger">Car</button>
                  </div>

                </div>
                <div class="col">
                  <div class="search-wrapper float-end">
                    <div class="input-holder">
                      <input type="text" class="search-input" placeholder="Type to search">
                      <button class="search-icon"><span></span></button>
                    </div>
                    <button class="close"></button>
                  </div>
                </div>
              </div>
              <div class="table-responsive">
                <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                  <thead>
                    <tr>
                      <th class="text-center">#</th>
                      <th class="text-center">Name/Number</th>
                      <th class="text-center">Type</th>
                      <th class="text-center">Vehicle No</th>
                      <th class="text-center">In time</th>
                      <th class="text-center">Out time</th>
                      <th class="text-center">Total Hours</th>
                      <th class="text-center">Charges</th>
                      <th class="text-center">Status</th>
                      <th class="text-center">Actions(User)</th>
                    </tr>
                  </thead>
                  <% @vehicles.each do |vehicle| %>
                  <%= render vehicle %>
                  <%# <td>
                    <p>
                      <%= link_to "Show this vehicle", vehicle %>
                    <%# </p>
                  </td>  %>
                  <% end %>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
